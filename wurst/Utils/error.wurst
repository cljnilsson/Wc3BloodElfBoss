package error

sound error

public function player.error(string msg)
    string s =  "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n|cffffcc00"+msg+"|r"
    if localPlayer == this
        ClearTextMessages()
        DisplayTimedTextToPlayer(this, 0.52, 0.96, 2.00, s)
        StartSound(error)

init
    error=CreateSoundFromLabel("InterfaceError",false,false,false,10,10)
